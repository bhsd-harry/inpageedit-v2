{
  "name": "mediawiki-inpageedit",
  "version": "14.2.2-beta.0",
  "description": "A useful MediaWiki JavaScript Plugin written with jQuery",
  "main": "dist/InPageEdit.js",
  "dependencies": {},
  "devDependencies": {
    "@dragon-fish/bump": "^0.0.15",
    "eslint": "^7.32.0",
    "prettier": "^2.4.0",
    "types-mediawiki": "^1.2.0",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "build": "yarn clear && webpack && wsl MINIFY=1 webpack",
    "lint": "eslint ./src",
    "dev": "webpack --watch --o ./dev/[name].js",
    "clear": "wsl rm -rf ./dist",
    "pretty": "prettier --write ./src",
    "bump": "yarn lint && bump --nopublish && yarn build && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inpageedit/inpageedit-v2.git"
  },
  "keywords": [
    "mediawiki",
    "mediawiki-gadget",
    "inpageedit"
  ],
  "author": "Dragon-Fish",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/inpageedit/inpageedit-v2/issues"
  },
  "homepage": "https://www.ipe.wiki/"
}
